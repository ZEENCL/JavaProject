public class JavaStatements
{
	private static int[] num = {0, 10, 15 ,54, 15, 15, 10, 10};
	
	public static void main(String[] args)
	{
		for (int i = 0; i < num.length; i++)
		{
			if (num[i] == 10)
			{
				System.out.println(num[i] + "This is number five!");
			}
			else
			{
				System.out.println(num[i] + "I don't like this number!");
			}
		}
	}
}